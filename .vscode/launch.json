  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Harness",
      "program": "${workspaceFolder}/harness.mjs",
      "args": [
        "--trace-warnings",
        "--inspect"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "test",
      "program": "${workspaceFolder}/test.js"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Mocha",
      "program": "${workspaceFolder}/node_modules/mocha/bin/_mocha",
      "args": [
        "--colors",
        "--timeout",
        "999999",
        "${workspaceFolder}/test"
      ]
    }
  ]
}